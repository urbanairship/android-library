<?xml version='1.0' encoding='utf-8'?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Accessibility -->
    <!-- Read aloud by screen readers to confirm that selected messages have been marked as read. -->
    <plurals name="ua_mc_description_marked_read">
        <item quantity="one">Pažymėta 
        <xliff:g example="1" id="count">%1$d</xliff:g>žinutė kaip perskaityta</item>
        <item quantity="few">Pažymėtos 
        <xliff:g example="7" id="count">%1$d</xliff:g>žinutės kaip perskaitytos</item>
        <item quantity="many">Pažymėtos 
        <xliff:g example="7" id="count">%1$d</xliff:g>žinutės kaip perskaitytos</item>
        <item quantity="other">Pažymėtos 
        <xliff:g example="7" id="count">%1$d</xliff:g>žinutės kaip perskaitytos</item>
    </plurals>
    <!-- Read aloud by screen readers to confirm that selected messages have been marked as read. -->
    <plurals name="ua_mc_description_deleted">
        <item quantity="one">Ištrinta 
        <xliff:g example="1" id="count">%1$d</xliff:g>žinutė</item>
        <item quantity="few">Ištrintos 
        <xliff:g example="7" id="count">%1$d</xliff:g>žinutės</item>
        <item quantity="many">Ištrintos 
        <xliff:g example="7" id="count">%1$d</xliff:g>žinutės</item>
        <item quantity="other">Ištrintos 
        <xliff:g example="7" id="count">%1$d</xliff:g>žinutės</item>
    </plurals>
    <!-- Message item content descriptions. Used on the Inbox (message list) screen. -->
    <!-- Prepended to the content description for messages that are currently selected. -->
    <string name="ua_mc_description_state_selected">Pasirinkta,</string>
    <!-- Prepended to the content description for messages that are currently unread. -->
    <string name="ua_mc_description_state_unread">Neskaityta,</string>
    <!-- Content description for message items in the message list. Dates will be formatted using
    the DateFormat.LONG format in the current locale. -->
    <string name="ua_mc_description_title_and_date">
    <xliff:g example="This week\'s top deals" id="title">%1$s</xliff:g>, 
    <xliff:g example="May 1st, 2021" id="sent_date">%2$s</xliff:g></string>
    <!-- Message item actions. Displayed in TalkBack Actions menu when screen reader is active. -->
    <!-- Action that selects the focused message. -->
    <string name="ua_mc_action_select">Pasirinkti žinutę</string>
    <!-- Action that unselects the focused message. -->
    <string name="ua_mc_action_unselect">Panaikinti žinutės pasirinkimą</string>
    <!-- Custom message item tap action. Will be spoken as "Tap to read message" in EN-US. -->
    <string name="ua_mc_action_click">Skaityti žinutę</string>

    <!-- Accessibility -->
    <string name="ua_score_selected_state_description">%1$d iš %2$d</string>
    <string name="ua_selected">Pasirinkta</string>
    <string name="ua_not_selected">Nepasirinkta</string>
</resources>
