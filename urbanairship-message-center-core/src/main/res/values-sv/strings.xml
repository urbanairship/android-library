<?xml version='1.0' encoding='utf-8'?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Accessibility -->
    <!-- Read aloud by screen readers to confirm that selected messages have been marked as read. -->
    <plurals name="ua_mc_description_marked_read">
        <item quantity="one">Markerade 
        <xliff:g example="1" id="count">%1$d</xliff:g>meddelande som läst</item>
        <item quantity="other">Markerade 
        <xliff:g example="7" id="count">%1$d</xliff:g>meddelanden som lästa</item>
    </plurals>
    <!-- Read aloud by screen readers to confirm that selected messages have been marked as read. -->
    <plurals name="ua_mc_description_deleted">
        <item quantity="one">Raderade 
        <xliff:g example="1" id="count">%1$d</xliff:g>meddelande</item>
        <item quantity="other">Raderade 
        <xliff:g example="7" id="count">%1$d</xliff:g>meddelanden</item>
    </plurals>
    <!-- Message item content descriptions. Used on the Inbox (message list) screen. -->
    <!-- Prepended to the content description for messages that are currently selected. -->
    <string name="ua_mc_description_state_selected">Valda,</string>
    <!-- Prepended to the content description for messages that are currently unread. -->
    <string name="ua_mc_description_state_unread">Oläst,</string>
    <!-- Content description for message items in the message list. Dates will be formatted using
    the DateFormat.LONG format in the current locale. -->
    <string name="ua_mc_description_title_and_date">
    <xliff:g example="This week\'s top deals" id="title">%1$s</xliff:g>, den 
    <xliff:g example="May 1st, 2021" id="sent_date">%2$s</xliff:g></string>
    <!-- Message item actions. Displayed in TalkBack Actions menu when screen reader is active. -->
    <!-- Action that selects the focused message. -->
    <string name="ua_mc_action_select">Välj meddelande</string>
    <!-- Action that unselects the focused message. -->
    <string name="ua_mc_action_unselect">Avmarkera meddelande</string>
    <!-- Custom message item tap action. Will be spoken as "Tap to read message" in EN-US. -->
    <string name="ua_mc_action_click">Läs meddelande</string>

    <!-- Accessibility -->
    <string name="ua_score_selected_state_description">%1$d av %2$d</string>
    <string name="ua_selected">Vald</string>
    <string name="ua_not_selected">Inte vald</string>
</resources>
