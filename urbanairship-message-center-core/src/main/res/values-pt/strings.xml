<?xml version='1.0' encoding='utf-8'?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Accessibility -->
    <!-- Read aloud by screen readers to confirm that selected messages have been marked as read. -->
    <plurals name="ua_mc_description_marked_read">
        <item quantity="one">
        <xliff:g example="1" id="count">%1$d</xliff:g>mensagem marcada como lida</item>
        <item quantity="many">
        <xliff:g example="7" id="count">%1$d</xliff:g>mensagens marcadas como lidas</item>
        <item quantity="other">
        <xliff:g example="7" id="count">%1$d</xliff:g>mensagens marcadas como lidas</item>
    </plurals>
    <!-- Read aloud by screen readers to confirm that selected messages have been marked as read. -->
    <plurals name="ua_mc_description_deleted">
        <item quantity="one">
        <xliff:g example="1" id="count">%1$d</xliff:g>mensagem excluída</item>
        <item quantity="many">
        <xliff:g example="7" id="count">%1$d</xliff:g>mensagens excluídas</item>
        <item quantity="other">
        <xliff:g example="7" id="count">%1$d</xliff:g>mensagens excluídas</item>
    </plurals>
    <!-- Message item content descriptions. Used on the Inbox (message list) screen. -->
    <!-- Prepended to the content description for messages that are currently selected. -->
    <string name="ua_mc_description_state_selected">Selecionada,</string>
    <!-- Prepended to the content description for messages that are currently unread. -->
    <string name="ua_mc_description_state_unread">Não lida,</string>
    <!-- Content description for message items in the message list. Dates will be formatted using
    the DateFormat.LONG format in the current locale. -->
    <string name="ua_mc_description_title_and_date">
    <xliff:g example="This week\'s top deals" id="title">%1$s</xliff:g>, em 
    <xliff:g example="May 1st, 2021" id="sent_date">%2$s</xliff:g></string>
    <!-- Message item actions. Displayed in TalkBack Actions menu when screen reader is active. -->
    <!-- Action that selects the focused message. -->
    <string name="ua_mc_action_select">Selecionar mensagem</string>
    <!-- Action that unselects the focused message. -->
    <string name="ua_mc_action_unselect">Desmarcar mensagem</string>
    <!-- Custom message item tap action. Will be spoken as "Tap to read message" in EN-US. -->
    <string name="ua_mc_action_click">Ler a mensagem</string>

    <!-- Accessibility -->
    <string name="ua_score_selected_state_description">%1$d de %2$d</string>
    <string name="ua_selected">Selecionado</string>
    <string name="ua_not_selected">Não selecionado</string>
</resources>
