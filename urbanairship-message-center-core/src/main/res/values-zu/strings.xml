<?xml version='1.0' encoding='utf-8'?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Accessibility -->
    <!-- Read aloud by screen readers to confirm that selected messages have been marked as read. -->
    <plurals name="ua_mc_description_marked_read">
        <item quantity="one">Imakwe umlayezo ongu- 
        <xliff:g example="1" id="count">%1$d</xliff:g>njengofundiwe</item>
        <item quantity="other">Imakwe imilayezo engu- 
        <xliff:g example="7" id="count">%1$d</xliff:g>njengefundiwe</item>
    </plurals>
    <!-- Read aloud by screen readers to confirm that selected messages have been marked as read. -->
    <plurals name="ua_mc_description_deleted">
        <item quantity="one">Umlayezo osusiwe ongu- 
        <xliff:g example="1" id="count">%1$d</xliff:g></item>
        <item quantity="other">Imilayezo engu- 
        <xliff:g example="7" id="count">%1$d</xliff:g>esusiwe</item>
    </plurals>
    <!-- Message item content descriptions. Used on the Inbox (message list) screen. -->
    <!-- Prepended to the content description for messages that are currently selected. -->
    <string name="ua_mc_description_state_selected">Ikhethiwe,</string>
    <!-- Prepended to the content description for messages that are currently unread. -->
    <string name="ua_mc_description_state_unread">Akufundiwe,</string>
    <!-- Content description for message items in the message list. Dates will be formatted using
    the DateFormat.LONG format in the current locale. -->
    <string name="ua_mc_description_title_and_date">
    <xliff:g example="This week\'s top deals" id="title">%1$s</xliff:g>, ngomhla ka- 
    <xliff:g example="May 1st, 2021" id="sent_date">%2$s</xliff:g></string>
    <!-- Message item actions. Displayed in TalkBack Actions menu when screen reader is active. -->
    <!-- Action that selects the focused message. -->
    <string name="ua_mc_action_select">Khetha umlayezo</string>
    <!-- Action that unselects the focused message. -->
    <string name="ua_mc_action_unselect">Susa ukukhetha umlayezo</string>
    <!-- Custom message item tap action. Will be spoken as "Tap to read message" in EN-US. -->
    <string name="ua_mc_action_click">Funda umlayezo</string>

    <!-- Accessibility -->
    <string name="ua_score_selected_state_description">%1$d kwa %2$d</string>
    <string name="ua_selected">Kukhethiwe</string>
    <string name="ua_not_selected">Akukhethiwe</string>
</resources>
