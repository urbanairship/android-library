<?xml version='1.0' encoding='utf-8'?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Accessibility -->
    <!-- Read aloud by screen readers to confirm that selected messages have been marked as read. -->
    <plurals name="ua_mc_description_marked_read">
        <item quantity="zero">تم وضع علامة على 
        <xliff:g example="7" id="count">%1$d</xliff:g>رسائل كمقروءة</item>
        <item quantity="one">تم وضع علامة \"مقروءة\" على رسالة 
        <xliff:g example="1" id="count">%1$d</xliff:g></item>
        <item quantity="two">تم وضع علامة على 
        <xliff:g example="7" id="count">%1$d</xliff:g>رسائل كمقروءة</item>
        <item quantity="few">تم وضع علامة على 
        <xliff:g example="7" id="count">%1$d</xliff:g>رسائل كمقروءة</item>
        <item quantity="many">تم وضع علامة على 
        <xliff:g example="7" id="count">%1$d</xliff:g>رسائل كمقروءة</item>
        <item quantity="other">تم وضع علامة على 
        <xliff:g example="7" id="count">%1$d</xliff:g>رسائل كمقروءة</item>
    </plurals>
    <!-- Read aloud by screen readers to confirm that selected messages have been marked as read. -->
    <plurals name="ua_mc_description_deleted">
        <item quantity="zero">تم حذف 
        <xliff:g example="7" id="count">%1$d</xliff:g>رسائل</item>
        <item quantity="one">تم حذف رسالة 
        <xliff:g example="1" id="count">%1$d</xliff:g></item>
        <item quantity="two">تم حذف 
        <xliff:g example="7" id="count">%1$d</xliff:g>رسائل</item>
        <item quantity="few">تم حذف 
        <xliff:g example="7" id="count">%1$d</xliff:g>رسائل</item>
        <item quantity="many">تم حذف 
        <xliff:g example="7" id="count">%1$d</xliff:g>رسائل</item>
        <item quantity="other">تم حذف 
        <xliff:g example="7" id="count">%1$d</xliff:g>رسائل</item>
    </plurals>
    <!-- Message item content descriptions. Used on the Inbox (message list) screen. -->
    <!-- Prepended to the content description for messages that are currently selected. -->
    <string name="ua_mc_description_state_selected">تم تحديد</string>
    <!-- Prepended to the content description for messages that are currently unread. -->
    <string name="ua_mc_description_state_unread">غير مقروء</string>
    <!-- Content description for message items in the message list. Dates will be formatted using
    the DateFormat.LONG format in the current locale. -->
    <string name="ua_mc_description_title_and_date">
    <xliff:g example="This week\'s top deals" id="title">%1$s</xliff:g>في 
    <xliff:g example="May 1st, 2021" id="sent_date">%2$s</xliff:g></string>
    <!-- Message item actions. Displayed in TalkBack Actions menu when screen reader is active. -->
    <!-- Action that selects the focused message. -->
    <string name="ua_mc_action_select">حدد الرسالة</string>
    <!-- Action that unselects the focused message. -->
    <string name="ua_mc_action_unselect">قم بإلغاء تحديد الرسالة</string>
    <!-- Custom message item tap action. Will be spoken as "Tap to read message" in EN-US. -->
    <string name="ua_mc_action_click">اقرأ الرساله</string>
</resources>
