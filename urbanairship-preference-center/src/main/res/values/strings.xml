<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="ua_preference_center_label">Preference Center</string>
    <string name="ua_preference_center_empty">Unable to load preferences. Please try again later.</string>
    <string name="ua_preference_center_error_retry">@string/ua_retry_button</string>
    <string name="ua_preference_center_item_error">Update failed. Please try again later.</string>

    <!-- Strings below this comment need to be translated in the next localization update. -->

    <!-- Preference Center subscription item accessibility description. -->
    <!-- Examples: -->
    <!-- "Breaking News. Stay up to date with the latest news as it happens. Subscribed." -->
    <!-- "Today's Deals. Get notified about the best deals of the day. Unsubscribed." -->
    <string name="ua_preference_center_subscription_item_description">
        <xliff:g id="title" example="Today's Deals">%1$s</xliff:g>. <xliff:g id="description" example="Notify me about the best deals of the day">%2$s</xliff:g>. <xliff:g id="state" example="Subscribed">%3$s</xliff:g>.</string>
    <!-- State descriptions used in ua_preference_center_subscription_item_description -->
    <string name="ua_preference_center_subscribed_description">subscribed</string>
    <string name="ua_preference_center_unsubscribed_description">unsubscribed</string>

    <!-- Preference Center subscription item accessibility click action. -->
    <!-- Example: "Tap to subscribe" / "Tap to unsubscribe" -->
    <string name="ua_preference_center_action_subscribe">subscribe</string>
    <string name="ua_preference_center_action_unsubscribe">unsubscribe</string>

    <!-- Contact channel management -->
    <string name="ua_preference_center_contact_management_pending">Pending verification</string>
    <string name="ua_preference_center_contact_management_sms_description">Phone number</string>
    <string name="ua_preference_center_contact_management_email_description">Email address</string>
    <string name="ua_preference_center_contact_management_resend_description">Resend verification</string>
    <string name="ua_preference_center_contact_management_redacted_description">redacted</string>
