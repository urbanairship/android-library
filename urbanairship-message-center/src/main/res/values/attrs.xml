<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Style attributes for Message Center -->
    <declare-styleable name="UrbanAirship.MessageCenter">

        <!-- General Configuration -->
        <attr name="messageCenterToolbarTitle" format="reference|string" />

        <!-- Message List -->
        <attr name="messageCenterIconsEnabled" format="boolean" />
        <attr name="messageCenterPlaceholderIcon" format="reference" />
        <attr name="messageCenterItemDividersEnabled" format="boolean" />
        <attr name="messageCenterItemDividerInsetStart" format="dimension" />
        <attr name="messageCenterItemDividerInsetEnd" format="dimension" />
    </declare-styleable>
</resources>
